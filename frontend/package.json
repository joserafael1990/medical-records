{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@amplitude/analytics-browser": "^2.31.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@mui/x-date-pickers": "^8.10.2",
    "@nivo/core": "^0.99.0",
    "@nivo/sankey": "^0.99.0",
    "@sentry/react": "^8.55.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/date-fns": "^2.5.3",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-window": "^1.8.8",
    "axios": "^1.11.0",
    "d3-interpolate": "^3.0.1",
    "d3-scale-chromatic": "^3.1.0",
    "date-fns": "^4.1.0",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.1",
    "react-scripts": "5.0.1",
    "react-window": "^1.8.11",
    "recharts": "^3.4.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "SKIP_PREFLIGHT_CHECK=true TSC_COMPILE_ON_ERROR=true DISABLE_ESLINT_PLUGIN=true GENERATE_SOURCEMAP=false react-scripts start",
    "build": "react-scripts build",
    "test": "jest --forceExit --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --forceExit",
    "test:ci": "jest --ci --coverage --watchAll=false --forceExit --maxWorkers=2",
    "test:cleanup": "pkill -f jest-worker || true",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "analyze:sizes": "node scripts/analyze-file-sizes.js",
    "build:analyze": "ANALYZE=true npm run build",
    "optimize": "npm run analyze:sizes && npm run lint:fix && npm run type-check",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "@babel/preset-typescript": "^7.23.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "babel-jest": "^29.7.0",
    "babel-plugin-import": "^1.13.8",
    "baseline-browser-mapping": "^2.9.12",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
